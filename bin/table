#!/usr/bin/python3 

txt = """# BOTD - python3 IRC channel daemon\n#\n# dispatch tables.\n"""

import os, sys ; sys.path.insert(0, os.getcwd())

import json
import time
import botd.tbl

from botd.log import level
level("error")

botd.tbotd.classes = []
botd.tbotd.names = {}
botd.tbotd.modules = {}

from botd.krn import Kernel

k = Kernel()
k.walk("bl")
k.walk("botd")
print(txt)
print("classes = %s" % botd.tbotd.classes)
print("names = %s" % botd.tbotd.names)
print("modules = %s" % botd.tbotd.modules)
sys.exit()
