#!/usr/bin/python3 

txt = """# BOTD - python3 IRC channel daemon\n#\n# dispatch tables.\n"""

import os, sys ; sys.path.insert(0, os.getcwd())

import json
import time
import botd.tbl

from botd.log import level
level("debug")

botd.tbl.classes = []
botd.tbl.names = {}
botd.tbl.modules = {}

from botd.krn import Kernel

k = Kernel()
k.walk("botd")

print(txt)
print("classes = %s" % botd.tbl.classes)
print("names = %s" % botd.tbl.names)
print("modules = %s" % botd.tbl.modules)
sys.exit()
