#!/usr/bin/python3 

txt = """# BOTD - python3 IRC channel daemon\n#\n# dispatch tables.\n"""

import os, sys ; sys.path.insert(0, os.getcwd())

import json
import time
import bl.tbl

from bl.log import level
level("error")

if not getattr(bl.tbl, "classes", None):
    bl.tbl.classes = []
if not getattr(bl.tbl, "names", None):
    bl.tbl.names = {}
if not getattr(bl.tbl, "modules", None):
    bl.tbl.modules = {}

from bl.krn import Kernel

k = Kernel()
k.load_mod("botd.cmd")
print(txt)
print("classes = %s" % bl.tbl.classes)
print("names = %s" % bl.tbl.names)
print("modules = %s" % bl.tbl.modules)
sys.exit()
